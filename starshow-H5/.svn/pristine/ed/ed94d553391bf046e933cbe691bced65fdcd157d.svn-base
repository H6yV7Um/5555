body{
	    min-width: 1200px;
}
.xingx_top{
	width: 100%;
	background: #2c2c2c;
	color: #ffffff;
}
.xingx_top a{
	color: #ffffff;
	text-decoration: none;
}
.xingx_top .topMain{
	background: #ff1d3e;
	height: 43px;
	line-height: 43px

}
.xingx_top .topMain .topCon{
	max-width: 1200px;
    position: relative;
    margin: 0 auto;
}
.xingx_top .topMain .link{
	margin-right: 220px;
}
.xingx_top .topMain .link a{
	margin-left:40px;
}
.xingx_top .topMain .platList{
	top:0;
	right: 0
}
.xingx_top .xingx_header{
	width: 820px;
	position: static;
	margin: 0 auto;
	background: #2c2c2c;
}

.xingx_top .xingx_header .xingx_content{
   position: static;
}
.xingx_top .xingx_header .childsMain {
	width: 100%;
    background: rgba(0,0,0,0.5);
    width: 100%;
    position: absolute;
    margin-top: 28px;
    left: 0;
    z-index: 999;
    color: #fff;
    text-align: center;
}
.xingx_top .xingx_header .childsMain  li a{
	color: #fff
}

.xingx_top .xingx_header .menu_childs{
	width: 1020px;
    position: static;
    opacity: 0.8;
    padding: 15px 0;
    margin: 0 auto;
    overflow: hidden;
    background: none;
}
.xingx_top .xingx_header .menu_childs li{
	display:inline;
	padding:0 5px;
	padding: 0 18px;
    width: auto;
    float:none;
}
.xingx_top .xingx_header .icon_logo,.xingx_top .xingx_header .platList,.xingx_top .xingx_header .xingx_other{
	display: none;
}
.xingx_top  .xingx_menus{
	width: 90%;
	margin: 0 auto;
	height:auto;
	line-height: 100%;
	position: static;

}
.xingx_top .xingx_menus .xingx_nav {
    width:100%;
    margin:0 auto;
    float: none;
}
.xingx_top .xingx_menus .xingx_nav > li{
	width: 14.28%;
	padding: 15px 0

}
.xingx_top .xingx_menus .xingx_nav > li:hover > a{
	color: #ff1d3e;
}
.xingx_top .xingx_menus .xingx_nav > li:hover .line{
	display: block;
}
.xingx_top .xingx_menus .xingx_nav > li.cur >a{
	color: #ff1d3e;
}
.xingx_top .xingx_menus .xingx_nav > li.cur .line{
	display: block;
}
.xingx_top .xingx_nav > li:first-child{
	display: block;
}

.xingx_top .xingx_menus .line {
    width: 100%;
    height: 2px;
    bottom: -13px;
    left: 0;
    right: 0;
    margin: auto;
    position: absolute;
    background: #ff1d3e;
    /* display: none; */
}
.focus{
	position: relative;
	overflow: hidden;
	background: #000;
	margin-top: 0;
}
.focus .center{
	position: relative;
	width: 820px;
	/*height: 550px;*/
	margin:0 auto;
}
.focus .center .mask-left,
.focus .center .mask-right{
	position: absolute;
	top:0;
	width: 1000%;
	/*height: 550px;*/
	z-index: 1;
	background: rgba(0,0,0,0.6);
}
.focus .center .mask-left{
	right:100%;
}
.focus .center .mask-right{
	left:100%;
}
.focus .center a >img{
	display: block;
	width: 820px;
	/*height: 550px;*/
}
.focus .swiper-container{
	overflow: visible;
}
.focus .btn-left{
	left:-140px;
	z-index: 2;
	background:url(../images/icon-left2.png);
}
.focus .btn-right{
	right:-140px;
	background:url(../images/icon-right2.png);
	z-index: 2;
}
.focus .center:hover .btn-left,
.focus .center:hover .btn-right{
	display: block;
	z-index: 2;
}
.focus  .btn-left, .btn-right { 
	display: none; 
    position: absolute;
    width: 40px;
    height: 103px;
    top:0;
    bottom: 0;
    margin:auto;
    cursor: pointer;
}

.focus .swiper-pagination-bullet {
    width:20px;
    height: 8px;
    display: inline-block;
    border-radius: 0;
}
.focus_tit{
	position: absolute;
	bottom: 60px;
	width: 100%;
	text-align: center;
	color: #ffffff;
	/* background: rgba(0,0,0,0.5); */
	background: url(../../assets/images/pic/bg.png) no-repeat center center;
	height: 60px;
    line-height: 60px;

}
.xingx_cons li img,.swiper-container-rank img,.swiper-container-tv img{
	-webkit-transition:transform 400ms;
       -moz-transition:transform 400ms;
       -ms-transition:transform 400ms;
         -o-transition:transform 400ms;
    	    transition:transform 400ms;
}
.xingx_cons  li:hover img,.swiper-container-rank .swiper-slide:hover img{
	-webkit-transform:scale(1.03,1.03);
	   -moz-transform:scale(1.03,1.03);
