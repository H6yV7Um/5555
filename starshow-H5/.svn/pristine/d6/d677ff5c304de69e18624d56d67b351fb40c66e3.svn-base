@charset "utf-8";
/* CSS Document */

body {margin: 0;padding: 0;-webkit-font-smoothing: antialiased;/*font-family: SourceHanSansCN-Light;*/}
.grayBg {background: #f3f3f3;}
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, button, textarea, p, blockquote, th, td, span {margin: 0;padding: 0;font-family: "Microsoft YaHei";}
h1, h2, h3, h4, h5, h6, input, textarea, select, button strong {font-size: 100%;font-weight: normal;}
div, p, ul, ol, li {margin: 0;padding: 0;}
li {list-style: none}
ul {list-style: none;}
html, body {font-size: 62.5%;}
a {text-decoration: none;}
input, button, textarea, select, optgroup, option {font-family: inherit;font-size: inherit;font-style: inherit;font-weight: inherit;}
input, button, textarea, select {
*font-size:100%;}
img {vertical-align: middle}
input[type="button"], input[type="submit"], input[type="reset"], input[type="text"], input[type="number"], select {-webkit-appearance: none;outline: 0;-webkit-tap-highlight-color: rgba(0,0,0,0);}
textarea {-webkit-appearance: none;outline: 0;}
/*首页*/
.banner {width: 90%;margin: 0 auto;position: relative}
.banner img {width: 100%;display: block;}
.banner a {position: absolute;left: 5%;bottom: 0;font-size: 1.4rem;color: #fff; width:90%; height:40px; line-height:40px}
.fl{float:left;}
.fr{float:right;}
.content {padding-bottom: 5rem;}
.tit {margin-top: 2.2rem;font-size: 1.6rem;color: #ff1d3e;font-weight: bold;overflow: hidden}
.tit i {float: left;width: 15%;display: inline-block;margin-right: 1.7%}
.tit i img {width: 100%;display: block;}
.con {width: 91%;margin: 1rem auto;}
.con h2 {font-size: 1.7rem;font-weight: bold;line-height: 4rem;}
.con p {font-size: 1.5rem;line-height: 2.2rem}
.con p span{color:#f00;}
.con p a{color:#000;}
.btn, .notSucc {width: 100%;height: 4.8rem;line-height: 4.8rem;position: fixed;bottom: 0;left: 0;background: #000;text-align: center;font-size: 1.6rem}
.btn a {color: #fff;}
.notSucc a {float: left;width: 50%;border-right: 1px solid #fff;display: inline-block;box-sizing: border-box;color: #fff}
/*确认订单*/
.sureOrder h2 {height: 4rem;line-height: 4rem;text-align: center;font-size: 1.2rem;color: #999}
.userInfo {width: 92%;margin: 0 auto;font-size: 1.5rem;background: #fff;padding: 0 4%}
.userInfo dl {height: 5rem;line-height: 5rem;border-bottom: 1px solid #dbdbdb;overflow: hidden}
.userInfo dt {width: 30%;float: left;color: #000}
.userInfo dd {width: 62%;float: left;color: #666}
.userInfo dd input {height: 3rem;line-height: 3rem;border: 0;width: 100%;padding-top: 1rem}
.userInfo dd a{color:#000;}
/*购买详情*/
/*订单确认页面*/
.st_orderC_where {overflow: hidden;width: 100%;box-sizing: border-box;border: 1px solid #dbdbdb;}
.st_orderC_where .st_where_tit {color: #999;font-size: 1.2rem;text-align: center;padding: 15px 0;}
.st_orderC_where .st_where_tit span {color: #ff1d3e;}
.st_orderC_where .st_where_wrap {width: 94%;background: #fff;padding: 15px 4%;overflow: hidden}
.st_orderC_where .st_where_img {width: 22%;overflow: hidden;float: left;}
.st_orderC_where .st_where_img img {width: 100%;float: left;}
.st_orderC_where .st_where_txt {float: left;margin-left: 2%;width: 76%;}
.st_orderC_where .st_where_txt h2 {font-size: 1.6rem;padding-bottom: 5px;color: #000}
.st_orderC_where .st_where_txt p {font-size: 1.2rem;color: #444;}
.st_orderC_detail {font-size: 12px;padding-left: 15px;background-color: #fff;border-bottom: 1px solid #dbdbdb;}
.st_orderC_detail label {color: #444;}
.st_orderC_detail .time {float: left;overflow: hidden;width: 79px;}
.st_orderC_detail .time_list {margin-right: 15px;text-align: right;}
.st_orderC_detail li {padding: 10px 0;border-bottom: 1px solid #dbdbdb;}
.st_orderC_detail li span {float: right;margin-right: 15px;}
.st_orderC_detail li .sum {font-size: 14px;}
.st_orderC_detail li .money {font-size: 16px;}
.st_orderC_detail li:nth-child(4) {border-bottom: 0;}
.st_orderC_pay p {font-size: 1.2rem;color: #444;padding: 9.5rem 15px 5px;border-bottom: 1px solid #dbdbdb;}
.st_orderC_pay p span {color: #ff1d3e;font-size: 1.8rem;font-weight: bold;}
.st_orderC_pay ul {padding-left: 15px;border-bottom: 1px solid #dbdbdb;font-size: 14px;}
.st_orderC_pay h6 {font-size: 12px;color: #fa35a2;padding-left: 15px;padding-top: 13px;}
.st_orderC_pay h6 a {color: #fa35a2;text-decoration: none;}
.st_orderC_pay .zhifubao:before {content: "";width: 35px;height: 35px;display: inline-block;background: url(../images/zhifubao.png) no-repeat;background-size: 100%;vertical-align: middle;margin-right: 10px;}
.st_orderC_pay .weixin:before {content: "";width: 35px;height: 35px;display: inline-block;background: url(../images/weixin.png) no-repeat;background-size: 100%;vertical-align: middle;margin-right: 10px;}
.st_orderC_pay li {padding: 10px 0;border-bottom: 1px solid #dbdbdb;}
.st_orderC_pay li .sum {font-size: 14px;}
.st_orderC_pay li .money {font-size: 16px;}
.st_orderC_pay li:nth-child(2) {border-bottom: 0;}
.st_orderC_pay .active span {float: right;margin-right: 15px;width: 22px;height: 18px;display: inline-block;background: url(../images/duihao.png) no-repeat;background-size: 100%;margin-top: 10px;}
.st_orderC_gopay {position: fixed;bottom: 0;height: 50px;line-height: 50px;width: 100%;background-color: #000;color: #fa35a2;box-sizing: border-box;}
.st_orderC_gopay p {display: inline-block;text-align: center;width: 50%;box-sizing: border-box;}
.st_orderC_gopay .gocolor.gopay {width: 100%;text-align: center;border-left: 0;}
@media screen and (max-width: 320px) {
.st_orderC_pay ul {padding-bottom: 50px;}
.st_orderD_con {padding-bottom: 60px;}
/* .st_buyD_top {
    height: 126px;
  }*/}
.kefu {padding-left: 15px;font-size: 14px;padding-bottom: 90px;}
.st_buyD_warp {padding-bottom: 55px;}
.kefu a {color: #000;text-decoration: none}
.st_orderC_pay {padding-bottom: 70px;}
.mt20 {margin-top: 20px;}
/*订单详情*/
.orderDetail {padding-bottom: 5rem}
.orderInfo {background: #f3f3f3;width: 100%;padding: 3rem 0;text-align: center}
.orderInfo h2 {color: #000;font-size: 1.5rem;}
.orderInfo p {font-size: 2rem;color: #ff1d3e;margin-top: 20px}
.paid {display: none;}
.noOrder p, .paid p {text-align: center;font-size: 1.4rem;padding: 20px 0}
#BOX_overlay {position: absolute;z-index: 100;top: 0px;left: 0px;background-color: #ccc;filter: alpha(opacity=70);-moz-opacity: 0.7;opacity: 0.7;}
.dialog_tip {background: #fff;text-align: left;border: 1px solid #dedede;font-size: 12px;width: 20rem;border-radius: 10px;-moz-border-radius: 10px;-webkit-border-radius: 10px;-o-border-radius: 10px;}
.js-close {background: #fff;float: right;height: 30px;padding: 5px 0 0 10px;color: #000;margin-right: 10px; font-size:1.4rem}
.messdivCons {width: 100%;overflow: hidden;font-size: 1.3rem;color: #000;text-align: center;padding: 20px 0 40px;}
