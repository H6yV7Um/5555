/*飘心*/
.heart_area{
    width: 50%;
    position: absolute;
    bottom: 90px;
    right: 10px;
    z-index: 2;
    overflow: hidden;
    height: 200px;
}
.heart {
    position:absolute;
    width:2rem;
    height:2rem;
}
.heart img {
    width:100%;
    height:auto;
    display:block;
}
.heart1 {
    bottom:-5rem;
    right:40px;
    animation: myfirst 5s linear infinite;
    z-index:2222;
    animation-delay:0s;
    -webkit-animation: myfirst 5s linear infinite;
    z-index:2222;
    -webkit-animation-delay:0s;
}
.heart2 {
    bottom:-5rem;
    right:40px;
    animation: myfirst2 5s 1s linear infinite;
    animation-delay:2s;
    -webkit-animation: myfirst2 5s 1s linear infinite;
    -webkit-animation-delay:1s;
}
.heart3 {
    bottom:-5rem;
    right:40px;
    animation: myfirst3 6.2s linear infinite;
    z-index:2222;
    animation-delay:3s;
    -webkit-animation: myfirst3 6.2s linear infinite;
    -webkit-animation-delay:1s;
}
.heart4 {
    bottom:-5rem;
    right:40px;
    animation: myfirst4 5.2s 0.2s linear infinite;
    z-index:2222;
    animation-delay:5s;
    -webkit-animation: myfirst4 5s 0.2s linear infinite;
    -webkit-animation-delay:2s;
}
.heart5 {
    bottom:-5rem;
    right:40px;
    animation: myfirst5 7.5s 1.2s linear infinite;
    animation-delay:3s;
}
@keyframes myfirst {
    from {
    bottom: -5rem;
    right:40px;
}
to {
    bottom: 20rem;
    right:69px;
}
}
@keyframes myfirst2 {
    from {
    bottom: -5rem;
    right:40px;
}
to {
    bottom: 20rem;
    right:15px;
}
}
@keyframes myfirst3 {
    from {
    bottom: -5rem;
    right:40px;
}
to {
    bottom: 20rem;
    right:30px;
}
}
@keyframes myfirst4 {
    from {
    bottom: -5rem;
    right:40px;
}
to {
    bottom: 20rem;
    right:50px;
}
}
@keyframes myfirst5 {
    from {
    bottom: -5rem;
}
to {
    bottom: 20rem;
}
}
