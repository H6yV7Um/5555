@charset "utf-8";
/* CSS Document */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary, time, mark, audio, video, input {
	margin: 0;
	padding: 0;
	border: none;
	outline: 0;
	font-size: 100%;
	font-family: simSun;
	vertical-align: baseline;
	font-family: "Microsoft YaHei"
}
* {
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}
html, body, form, fieldset, p, div, h1, h2, h3, h4, h5, h6 {
	-webkit-text-size-adjust: none;
}
html, body {
	font-size: 62.5%;
}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	font-family: arial, sans-serif;
	background: #f1f1f1;
}
.wbg {
	background: #fff;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after, q:before, q:after {
	content: '';
	content: none;
}
ins {
	text-decoration: none;
}
del {
	text-decoration: line-through;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
.clear {
	clear: both;
}
.pos {
	position: relative;
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.hide {
	display: none;
}
.head {
	position: relative;
	text-align: center;
}
.banner img {
	display: block;
	width: 100%;
}
nav {
}
#carousel {
	position: absolute;
	bottom: 0;
	width: 100%;
	height: 45px;
	line-height: 45px;
	overflow: hidden;
}
#carousel ul {
	position: relative;
}
#carousel li {
	float: left;
	padding: 0 20px;
	color: #fff;
	font-size: 16px;
	background: #000;
	margin-top: 10px;
	line-height: 40px
}
#carousel li.cur {
	background: #ff6161;
	margin-top: 0;
	height: 50px;
	line-height: 55px
}
.tab {
	height: 35px;
	line-height: 35px;
	border-bottom: 1px solid #e1e1e1;
	background: #fff;
	padding: 5px 0;
	position: relative;
}
.tab ul li {
	float: left;
	width: 50%;
	border-right: 1px solid #e1e1e1;
	box-sizing: border-box;
	text-align: center;
	color: #6d6d6d;
	position: relative;
	font-size: 1.8rem;
}
.tab ul li span {
	font-weight: bold;
}
.tab ul li.cur {
	color: #000;
}
.tab p {
	width: 40%;
	border-bottom: 2px solid #000;
	position: absolute;
	bottom: 0;
	left: 5%
}
.list {
}
.list dl {
	background: #fff;
	border: 1px solid #e7e7e7;
	float: left;
	width: 49%;
	box-sizing: border-box;
	margin-top: 2%;
}
.list dl.odd {
	margin-left: 2%;
}
.list dt {
	position: relative;
}
.list dt img {
	height: auto;
	width: 100%;
	display: block;
}
.list dd {
	padding: 4%;
	overflow: hidden
}
.head h2 {
	margin-top: 1rem;
	font-size: 1.5rem;
	color: #fff;
}
.headInfo {
	width: 100%;
	text-align: center;
	position: absolute;
	top: 0;
	left: 0;
}
.avatar {
	margin: 1.7rem auto 0;
	width: 22%;
	border: 2px solid #fff;
	border-radius: 100%;
	overflow: hidden;
}
.avatar img {
	width: 100%;
	border-radius: 100%;
	display: block;
}
.headPic {
	width: 28%;
}
.headPic img {
	width: 40px;
	height: 40px;
	border-radius: 40px;
}
.listInfo h2 {
	height: 24px;
	line-height: 24px;
	font-size: 1.4rem;
	color: #242424;
	overflow: hidden;
}
.listInfo {
	width: 65%;
	overflow: hidden;
}
.listInfo p {
	font-size: 1.2rem;
	color: #5f5f5f;
}
.profileBtn {
	position: absolute;
	top: 20px;
	right: 0;
}
.profileBtn a {
	background: #000;
	color: #fff;
	text-align: center;
	display: inline-block;
	padding: 3px 5px;
	font-size: 14px;
	text-decoration: none
}
.like {
	background: #000;
	background: rgba(0,0,0,.4);
	border-radius: 20px;
	position: absolute;
	top: 10px;
	right: 10px;
	color: #fff;
	display: inline-block;
	padding: 2px 10px;
	font-size: 1.2rem;
}
.like i, .rank i {
	background: url(../images/icon.png) no-repeat;
	width: 15px;
	height: 13px;
	background-size: 15px 26px;
	margin: 3px 5px 0 0;
	display: inline-block;
	float: left
}
.unlike i {
	background: url(../images/icon.png) no-repeat;
	background-size: 15px 26px;
	background-position: 0 -13px;
}
.rank {
	overflow: hidden;
	display: none
}
.rank a {
	text-decoration:none;
	color: #000;
}
.rank dl {
	border-bottom: 1px solid #cfcfcf;
	padding: 10px;
	background: #f1f1f1;
	line-height: 60px;
	height: 60px;
	overflow: hidden
}
.rank dl.bgred {
	background: #fff7f7;
}
.rank dt {
	position: relative;
}
.rank dt, .rank dt span {
	float: left;
	font-size: 1.4rem;
}
.rank dd {
	float: right;
	font-size: 1.2rem;
}
.rankImg {
	padding: 0 30px 0 10px;
}
.rankImg img {
	width: 50px;
	height: 50px;
	border-radius: 50px;
}
.rank i {
	margin-top: 23px;
}
.rankNumber {
	font-weight: bold;
}
.rankName {
	width: 150px;
	overflow: hidden;
	text-overflow: ellipsis;
}
.number1, .number2, .number3 {
	background: url(../images/number.png) no-repeat;
	position: absolute;
	top: -10px;
	left: -5px;
	width: 80px;
	height: 80px;
	background-size: 80px 240px
}
.number2 {
	background-position: 0 -80px;
}
.number3 {
	background-position: 0 -160px;
}
/*detail start*/
.detail {
	width: 98%;
	margin: 0 auto;
	padding-bottom: 70px
}
.detail ul li, .v ul li {
	width: 31%;
	display: block;
	float: left;
	box-sizing: border-box;
	margin: 1% 1%;
	overflow: hidden;
}
.detail ul li img, .v ul li img {
	width: 100%;
	height: auto;
	display: block;
}
.yq {
	display: none;
}
.rankState, .yq {
	width: 100%;
	height: 50px;
	line-height: 50px;
	font-size: 20px;
	background: #f15463;
	bottom: 0;
	left: 0;
	text-align: center;
	color: #fff;
	position:absolute;
}
.yq a {
	color: #fff;
	text-decoration: none
}
.rankSuc {
	position: fixed;
	width: 200px;
	height: 80px;
	line-height: 80px;
	background: #000;
	background: rgba(0,0,0,.8);
	left: 50%;
	top: 50%;
	margin: -40px 0 0 -100px;
	color: #fff;
	text-align: center;
	display: none;
	font-size: 16px
}
/*profile start*/
.profile {
	width: 94%;
	margin: 0 auto;
	padding-bottom: 1rem
}
.profile h2 {
	height: 3rem;
	line-height: 3rem;
	font-size: 14px;
	color: #000
}
.profile p {
	font-size: 12px;
	line-height: 24px;
}
.profile span {
	color: #F00;
}
/*help me rank*/
.helpBg {
	background: #d7d7d7 url(../images/helpBg.jpg) center center no-repeat;
	background-size: cover
}
.help {
	background: #fff;
}
.helpBanner {
	width: 100%;
	position: relative;
}
.helpBanner img {
	width: 100%;
	display: block;
}
.topPic {
	position: absolute;
	left: 50%;
	bottom: -25px;
	margin: 0 0 0 -40px;
}
.topPic img {
	width: 80px;
	height: 80px;
	border-radius: 80px;
	border: 3px solid #fff;
}
.name {
	text-align: center;
	height: 30px;
	line-height: 30px;
	margin-top: 30px;
	font-weight: bold
}
.info {
	text-align: center;
}
.info i {
	padding: 0 20px;
	font-style: normal
}
.rankBtn {
	width: 40%;
	margin: 20px auto;
}
.rankBtn img {
	width: 100%;
	display: block;
}
.activityDesc {
	width: 94%;
	margin: 10px auto;
	padding-bottom: 60px
}
.activityDesc h3 {
	height: 30px;
	line-height: 30px;
	background: url(../images/titBg.png) repeat-x;
	color: #5f5f5f;
	font-size: 20px
}
.activityDesc span {
	text-align: center;
	background: #d7d7d7;
	margin: 0 auto;
	display: block;
	width: 30%
}
.activityDesc p {
	color: #000;
	font-size: 16px;
	margin-top: 10px;
	line-height: 26px;
}
.download {
	background: #000;
	background: rgba(0,0,0,.8);
	width: 100%;
	text-align: center;
	position: fixed;
	left: 0;
	bottom: 0;
	height: 50px;
	line-height: 50px
}
.download img {
	width: 50%;
	display: block;
	margin: 10px auto;
}
.photoAlbum {
	width: 80%;
	margin: 30px auto 0;
	position: relative;
	height: 340px;
}
.photoAlbum img {
	border: 2px solid #fff;
	border-radius: 15px;
	display: block;
	-webkit-box-shadow: 0 0 10px rgba(140, 140, 140, .5);
	-moz-box-shadow: 0 0 10px rgba(140, 140, 140, .5);
	box-shadow: 0 0 10px rgba(140, 140, 140, .5);
}
.rotate0 {
	transform: rotate(1deg);
	-ms-transform: rotate(1deg); 	/* IE 9 */
	-moz-transform: rotate(1deg); 	/* Firefox */
	-webkit-transform: rotate(1deg); /* Safari 和 Chrome */
	-o-transform: rotate(1deg);
}
.rotate1 {
	transform: rotate(2deg);
	-ms-transform: rotate(2deg); 	/* IE 9 */
	-moz-transform: rotate(2deg); 	/* Firefox */
	-webkit-transform: rotate(2deg); /* Safari 和 Chrome */
	-o-transform: rotate(6deg);
}
.rotate2 {
	transform: rotate(3deg);
	-ms-transform: rotate(3deg); 	/* IE 9 */
	-moz-transform: rotate(3deg); 	/* Firefox */
	-webkit-transform: rotate(3deg); /* Safari 和 Chrome */
	-o-transform: rotate(3deg);
}
.rotate3 {
	transform: rotate(4deg);
	-ms-transform: rotate(4deg); 	/* IE 9 */
	-moz-transform: rotate(4deg); 	/* Firefox */
	-webkit-transform: rotate(4deg); /* Safari 和 Chrome */
	-o-transform: rotate(4deg);
}
.rotate5 {
	transform: rotate(5deg);
	-ms-transform: rotate(5deg); 	/* IE 9 */
	-moz-transform: rotate(5deg); 	/* Firefox */
	-webkit-transform: rotate(5deg); /* Safari 和 Chrome */
	-o-transform: rotate(5deg);
}
.rotate5 {
	transform: rotate(6deg);
	-ms-transform: rotate(6deg); 	/* IE 9 */
	-moz-transform: rotate(6deg); 	/* Firefox */
	-webkit-transform: rotate(6deg); /* Safari 和 Chrome */
	-o-transform: rotate(6deg);
}
.st_share {
	position: fixed;
	top: 0;
	left: 44%;
	width: 52%;
	z-index: 10;
	display: none;
}
.mask {
	width: 100%;
	background: #000;
	background: rgba(0,0,0,.8);
	position: absolute;
	top: 0;
	left: 0;
	z-index: 9;
}
.slide a.cla {
	position: relative;
	width: auto;
	height: auto
}
#YLimgwrap {
	position: fixed;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	background: rgba(0,0,0,.6);
	z-index: 3000;
	display: none;
}
#YLimgcon {
	display: inline-block;
	padding: 10px;
	position: absolute;
	width: 90%;
	left: 5%
}
#YLimgcon:hover {
	cursor: move;
}
#YLimgcon h4 {
	box-sizing: border-box;
	height: 25px;
	line-height: 20px;
	padding: 0 0 5px;
	font-size: 16px;
	font-weight: normal; /*background:#6CF*/
}
#YLimgcon h4 a {
	float: right;
	display: inline-block;
	width: 20px;
	height: 20px;
	text-align: center;
	margin-left: 6px;
	overflow: hidden;
	cursor: pointer;
	background: url(../images/live_close.png) no-repeat;
	background-size: 20px 20px;
	border-radius: 3px;
}
#YLimgcon h4 .YLt_close {
	background-position: right top;
}
#YLimgcon h4 .YLt_close:hover {
	background-position: right bottom;
}
#YLimgcon h4 .YLt_size {
	background-position: -38px top;
}
#YLimgcon h4 .YLt_size:hover {
	background-position: -38px bottom;
}
#YLimgcon .prev, #YLimgcon .next {
	display: inline-block;
	width: 40px;
	height: 40px;
	position: absolute;
	top: 50%;
	overflow: hidden;
	margin-top: -20px;
}
#YLimgcon .prev {
	left: 0;
	background: url(../images/ico_direction.png) left no-repeat;
}
#YLimgcon .next {
	right: 0;
	background: url(../images/ico_direction.png) right no-repeat;
}
#YLimgcon img {
	width: 100%;
}
.list01 {
	overflow: hidden;
}
.tit {
	width: 92%;
	padding: 1.6rem 4%;
	margin: 0 auto;
}
.tit img {
	width: 100%;
}
