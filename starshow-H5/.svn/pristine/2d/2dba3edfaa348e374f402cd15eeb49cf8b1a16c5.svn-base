@charset "utf-8";
/* CSS Document */

a, address, b, big, blockquote, body, center, cite, code, dd, del, div, dl, dt, em, fieldset, font, form, h1, h2, h3, h4, h5, h6, html, i, iframe, img, ins, label, legend, li, ol, p, pre, small, span, strong, u, ul, var ,input{
	margin: 0;
	padding: 0;
	font-family:"Microsoft YaHei"
}
button, html input[type="button"], input[type="submit"] {
	cursor: pointer;
}
button[disabled], html input[disabled] {
	cursor: default;
}
ol, ul {
	list-style: outside none none; list-style-type:none
}
ul li{list-style-type:none;}
img {
	border: 0 none;
	vertical-align: middle;
}
em, i, u {
	font-style: normal;
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.al {
	text-align: left;
}
.ac {
	text-align: center;
}
.ar {
	text-align: right;
}
.hide {
	display: none;
}
.overf{overflow:hidden;}
a{text-decoration:none;}
.clear{clear:both;}
.mt30{margin-top:30px;}
.mt80{margin-top:80px;}
