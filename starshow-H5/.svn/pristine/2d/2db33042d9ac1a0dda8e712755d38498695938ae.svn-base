
body,html{
	font-family: PingFangSC-Light;
	font-size: 62.5%;
 	-webkit-overflow-scrolling: touch;   
}
.col_red{
	color: #FF1D3E
}
.col_grey{
	color: #9B9B9B
}
.prod_main{
	width: 100%;

}
.prod_main_pic{
	width: 100%;	
}
.prod_main_pic_fixed{
	width: 100%;	
	position: fixed;
	top:0;
	z-index: -1;
	
}
.prod_main_pic_abs{
	width: 100%;	
	position: absolute;
	top:0;
	z-index: -1
}
.prod_detail{
	width: 100%;
	background: #fff;
	overflow: auto;
	padding-bottom: 80px;

}
.prod_detail_abs{
	width: 100%;	
	position: absolute;
	top:0;
	z-index: 1;
}
.prod_info{
	width: 100%;
	padding: 10px 0;
	/* margin-top: 10px; */
}

.prod_tit{
	line-height: 1.8rem;
	text-align: center;	
}
.prod_tit h1{
	width: 70%;
	margin:10px auto;
	font-size: 1.6rem;
	color: #000000;
	font-family: PingFangSC-Light;

	
}
.prod_tit h2{
	line-height: 40px;
	font-size: 24px;
	color: #9B9B9B;
	font-size: 1.2rem;

}
.real_price_num{
	text-decoration: line-through;
}
.prod_info_fixed{
	padding: 0;
	padding: 35px 0 10px 0;
	background: #fff;
	z-index: 2;
	position: fixed;
	top:0;
	box-shadow: 0px 1px 5px 0px #dedede;
	padding-top: constant(safe-area-inset-top);        
}
.prod_info_fixed.share{
	padding: 10px 0;
}
.prod_info_fixed .prod_tit h1{
	font-size: 1.4rem;
	margin: 0 auto;
}
.prod_info_fixed .prod_tit h2{
	display: none;
}
.prod_info_fixed .prod_price{
	line-height: 18px;
}
.prod_info_fixed .prod_price span{
	font-size: 1.2rem
}
.prod_price{
	width: 100%;
	text-align: center;
	color: #000;
	line-height: 30px;
}
.prod_price .sale_price{
	/*font-size: 1.9rem;*/
	color:#3b3b3b
}
.prod_price .sale_price .sale_price_num{
	font-size: 1.9rem;
}
.prod_price .prod_freight{
	font-size: 1.5rem;
}
.prod_price .real_price{
	font-size: 1.2rem;
	/*display:none*/
}
.rebate{
      color:#ff1d3e;
      vertical-align: middle;
      margin-left:10px;
      font-size:1.2rem;
}
.prod_desc{
	width:100%;
	overflow: hidden;
}
.dis_area{
    width: 90%;
    margin: 0px auto;
    position: fixed;
    bottom: 20px;
    left: 0;
    right: 0;
    background: #DADADA;
    padding-bottom: 12px;
    padding-top: 12px;
    z-index: 3;
    text-align: center;
    color: #fff;
    font-size: 1.4rem;
}
.btnList{
    display: -webkit-flex;
    display: flex;
    justify-content:space-around;
    -webkit-justify-content: space-around;
    -moz-justify-content: space-around;
    -o-justify-content: space-around;
    flex-wrap: nowrap;
    width: 100%;
    margin: 0px auto;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(255,255,255,0.9);
    padding-bottom:10px;
    padding-top: 10px;
    z-index: 3;
}
.btnList li{
	width: 44%;
	text-align: center;
	line-height: 4.5rem;
	color: #fff;
	font-size: 1.5rem;
	background: #FA8817;
	height: 4.5rem;
	border-radius: 5px;

}
.btnList li:last-child{
	background: #ff1d3e
}

.btn_choose{
	border: 1px solid #898989;
	border-radius: 6px;
	width: 81%;
	margin: 10px auto;
	height: 4.5rem;
	line-height: 4.5rem;
	padding-left: 5%;
	color: #898989;
	font-size: 1.5rem

}
.btn_choose .btn_right{
	width: 14%;
	float: right;
	height: 100%;
	vertical-align: middle;
	text-align: center;
	border-left: 1px solid #898989;
}
.btn_choose .btn_right img{
	width: 18px;
}
.prod_tabs{
	width:100%;
	margin: 40px 0;
}
.tab_title{
	font-size: 1.7rem;
    color: #000;
    overflow: hidden;
    border-bottom: 1px solid #f0f0f0;
    width: 90%;
    padding: 20px 5%;

}
.tab_title .fr.icon_arraw{
	width: 20px;
	margin-top: 5px;
}
.tab_con{
	width: 90%;
    padding: 20px 5%;
	font-size: 1.5rem;
	color: #000000;
	line-height: 2.3rem;
	display: none;
	border-bottom: 1px solid #f0f0f0;
}
.js_chose_noParam{
	color: #999;
	padding-top: 10px;
	position: absolute;
	top: 22px;
}
.prod_params{
	position: fixed;
	/* min-height: 20rem; */
	z-index: 4;
	background: #FFFFFF;
	/* box-shadow: 0px 1px 6px 2px rgba(0,0,0,0.5); */
	margin: auto;
	left: 0;
	right: 0;
	bottom: 0;
	display: none;

}
.showSweetAlert.black_style{
	background: rgba(0,0,0,0.8);
	color: #fff;
}
.showSweetAlert.black_style h2{
	color: #fff;
}
.prod_params .params_title{
	width: 100%;
	position: relative;
	padding-bottom:40px;
}
.prod_params .params_title .params_cover{
	width: 27%;
	height: 9rem;
	/* border: 1px solid #E3E3E3; */
	border-radius: 10px;
	position: absolute;
	top: -30px;
	left: 5%;
	overflow: hidden;
	text-align: center;

}
.prod_params .params_title .params_cover img{
	/*width: 100%; */
	vertical-align: middle;
	height: 100%;
}
.params_desc{
    width: 65%;
    margin-left: 36%;
   
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    justify-content: space-around;
    margin-top:5px;
}
.params_desc > div{

}
.params_desc .param_price{
	font-size: 1.7rem;
	
}
.params_desc .param_price .real_price_num{
	font-size: 1.2rem;
}
.params_desc .param_choose{
	font-size: 1.2rem;
	color: #808080;
	height: 1.4rem;
	margin-top:10px;
	
}
.params_desc .param_choose span{
 	padding-right:10px;
}

.params_desc .param_choose span:last-child:after{
	content:"";
}
.params_cons{
	overflow: hidden;
	/*margin-top: 40px;
	padding: 10px*/
}
.params_cons h1{
	font-size: 1.5rem;
	color: #000000;
}
.params_cons .param_lists{
	width: 100%;
	margin-top: 10px;
	font-size: 1.4rem;

}
.params_cons .param_lists span{
	background: #FFFFFF;
	border: 1px solid #E5E5E5;
	padding:3px 15px;
	display: inline-block;
	margin-right: 10px;
	margin-top: 5px;
}
.params_cons .param_lists span.cur{
	border: 1px solid #FF1D3E;
	color: #FF1D3E
}
.buy_num{
	width: 100%;
	overflow: auto;
	margin-top: 10px;
	padding-bottom:5rem;
}
.buy_num h1{
	float: left;
	line-height: 2.5rem;
}
.buy_buttons{
	float: right;
	margin-right:30px;
}
.buy_buttons span{
	border: 1px solid #E5E5E5;
	display: inline-block;
	text-align: center;
	width: 2.5rem;
	height: 2.5rem;
	line-height: 2.5rem;
	vertical-align: middle;
	font-size: 1.6rem

}
.txt_buyNum{
	width: 3.25rem;
	height: 2.5rem;
	border: 1px solid #E5E5E5;
	vertical-align: middle;
	margin: 0 4px;
	text-align: center;
	font-size:1.4rem;
}
.paramScroll{
	max-height: 350px; 
	padding:10px;
	overflow:auto
}
.type_lists{

}
.type_lists h1{
	margin-top: 10px;
}
.btn_sure{
	display: block;
	width: 85%;
	background:  #FF1D3E;
	border-radius: 6px;
	height: 4rem;
	line-height: 4rem;
	color: #fff;
	font-size: 1.6rem;
	border: none;
	/* position: absolute; */
	bottom:5px;
	left: 0;
	right: 0;
	margin:5px auto;
	text-align: center;
	letter-spacing: 20px;

}
.btn_sure_disabled{
	background:  #DEDEDE;
}
.info_con h1{
	font-size: 1.5rem;
	margin: 5px 0;
	font-weight: bold
}
.shopInfo{
	width: 100%;
	text-align: center;
}
.shop_cover{
	border: 1px solid #F0F0F0;
	border-radius: 50%
}
.shopInfo p img,.shopInfo p span{
	vertical-align: middle;
	margin-left: 5px;
	font-size: 1.4rem
}

.recommd_tit{
	background: #ebebeb;
	font-size: 1.7rem;
	/* color: #898989; */
	height: 4rem;
	line-height: 4rem;
	/* text-align: center; */
	position: relative;
	margin: 20px 0 0 0;
	padding-top: 15px;
}
.recommd_tit .line{
	width: 70%;
	height: 1px;
	background: #CCCCCC;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	display:none;
}
.recommd_tit span{
	display: inline-block;
	background: #fff;
	padding: 0 3%;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	z-index: 1;
	/* position: absolute; */
	/* width: 40%; */
	width: 94%;


}
.rec_list{
	display: flex;
	display:-webkit-flex;
	justify-content: space-around;
	-webkit-justify-content: space-around;
	flex-wrap: wrap;
	-webkit-flex-wrap:wrap;

}
.rec_list li{
	width: 50%;
	overflow: hidden;
	padding: 10px;
	box-sizing:border-box;
	border-bottom: 1px solid #F0F0F0;
}
.rec_list li a{
	text-decoration: none;
}
.rec_list li:nth-child(2n){
	border-left: 1px solid #F0F0F0;
}
.rec_list li .rec_cover{
	width: 100%;
}
.rec_list li .rec_tit{
	font-size: 1.5rem;
	color: #434343;
	line-height: 1.9rem;
	text-overflow: ellipsis;
	height: 3.8rem;
	overflow: hidden;
	text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.rec_price{
	font-size: 1.7rem;
	line-height: 2.4rem;
	color: #000000;
	vertical-align: middle;
}

.prod_desc_con img,.prod_desc_con div{
	max-width:100%;	
	height:auto;
}
.buyerList{
	width: 100%;
	position: absolute;
	top: 60px;
	left: 10px;
	z-index: 3;
	overflow: hidden;
	height: 34px;
	line-height: 32px;
	width: auto;
	padding: 0 10px;
	min-width: 150px;
	display: inline-block;
	border-radius: 20px;
	background: #999999;
	color: #fff;	
	text-align: center;
	font-size:1.4rem;

}
.buyerList ul{
	
}

.buyerList ul li img{
	width: 20px;
	vertical-align: middle;
	margin-right: 5px;
	border-radius:50%
}
.buyerList ul li span{
	vertical-align: middle;
}
