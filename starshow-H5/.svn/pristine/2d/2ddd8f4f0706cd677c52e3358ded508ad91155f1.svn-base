/*focus*/
.focus{
	position: relative;
	overflow: hidden;
	background: #000;
	margin-top: 0;
}
.focus .center{
	position: relative;
	/* width: 820px;
	height: 550px; */
	width: 920px;
	height: 595px;
	margin:0 auto;
}
.focus .center .mask-left,
.focus .center .mask-right{
	position: absolute;
	top:0;
	width: 1000%;
	height: 595px;
	z-index: 1;
	background: rgba(0,0,0,0.6);
}
.focus .center .mask-left{
	right:100%;
}
.focus .center .mask-right{
	left:100%;
}
.focus .center a >img{
	display: block;
	width: 920px;
	height:595px;
}
.focus .swiper-container{
	overflow: visible;
}
.focus .btn-left{
	left:-140px;
	z-index: 2;
	background:url(../images/icon-left2.png);
}
.focus .btn-right{
	right:-140px;
	background:url(../images/icon-right2.png);
	z-index: 2;
}
.focus .center .btn-left,
.focus .center .btn-right{
	display: block;
	z-index: 2;
}
.focus  .btn-left, .btn-right { 
	display: none; 
    position: absolute;
    width: 40px;
    height: 103px;
    top:0;
    bottom: 0;
    margin:auto;
    cursor: pointer;
}

.focus .swiper-pagination-bullet {
    width:20px;
    height: 8px;
    display: inline-block;
    border-radius: 0;
}
.focus_tit{
	position: absolute;
