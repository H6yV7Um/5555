html,body{
  height:100%
}
/* 星美图 */

.s-pics-detail{
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #000;
  position: relative;
  margin-top: 0;
}
.picDetailList li span {
    position: absolute;
    background: rgba(0,0,0,0.5);
    color: #fff;
    right: 0;
    bottom: 0;
    text-align: center;
    padding: 1px 10px;
}
.bigImg_tit{
    padding-top: 2%;
    padding-left: 1rem;
    position: absolute;
    z-index: 9;
}
.bigImg_tit img{
  width: 35px;
}
.s-pics-detail .swiper-container {
  width: 100%;
  height: 100%;
}
.s-pics-detail .swiper-slide {
  /* text-align: center; */
  font-size: 18px;

  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.picCon{
	width: 100%;
	top:0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	position: absolute;
	text-align: center;
    margin: 0 auto;
    position: relative;
}
.picCon img{
	width: 100%;
}
.picCon .good_tag{
	width: 20px;
    height: 20px;
    position: absolute;
    z-index: 9
}
.picCon .good_tag_bg.bgfadeIn{
	width: 40px;
    height: 40px;
    position: absolute;
    z-index: -1
     animation:mymove 2s 1;
	-webkit-animation:mymove 2s 1; /* Safari 和 Chrome */

}
.silibingsPic{
	width:100%;
	height:100%;
	background: #000;
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}
.silibingsPic div{
    width: 10rem;
    overflow: hidden;
    background: #A7A7A7;
    box-shadow: -3px -3px 0 2px #666;
    border-radius: 4px;
    position: relative;
    text-align: center;
    min-height: 9rem;


}
.silibingsPic div span{
  height: 1.4rem;
  position: absolute;
  top:0;left: 0;bottom: 0;right: 0;margin: auto;
  font-size: 1.4rem;
  color: #fff;
  font-weight: bold;
  text-shadow: 1px -1px 5px #000000;
}
.silibingsPic div img{
 opacity:0.8
}
.silibingsPic div.prev_pics{
 margin-right:5%
}
.silibingsPic div.next_pics{
  margin-left:5%;
}
.silibingsPic div img{
  width: 100%;
}
@keyframes mymove
{
  0% {background: rgba(255,255,255,0.8); border-radius: 50%}
  100% {background:rgba(255,255,255,0);border-radius: 50%;}
}
.good_main{
  
    padding: 10px 0;
    margin-top: 15px;
    position: absolute;
    bottom: 0;
    z-index: 999;
    width: 100%;
    display:none;
}
.goodCon{
  width: 91%;
    background: #fff;
    overflow: hidden;
    margin: 0 5px;
    font-size: 14px;
    position: relative;
    padding: 15px 2%;
}
.goodCon .good_pic_con{
  width:35%;
  float: left;
  display: inline;
}
.goodCon .good_desc{
  width: 63%;
    left: 37%;
    position: absolute;
    bottom: 20px;
    top: 20px;
}
.col_red{
  color: #ff2d3e
}
.good_btns{
  position: absolute;
  bottom: 0;
  width: 100%;
}
.goodCon img{
  width: 100%
}
.goodCon p.good_name{
  height: 50px;
    overflow: hidden;
    line-height: 24px;
    text-align: left;
    
}
.pic_nodata{
  position: absolute;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 100%;
  text-align: center;
  display: inline-block;
  color: #fff;
  font-size: 1.4rem;
  height:1.4rem
}

.bigImg_footer{
  position: absolute;
  bottom: 0px;
  height: 20px;
  background: rgba(0,0,0,0.6);
  color: #fff;
  width:98%;
  padding:5px 1%;
  font-size:14px;
  z-index:2
}