.loadding {
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.loadding .spinner {
    width: 150px;
    height: 80px;
    margin: 100px auto;
    text-align: center;
    -webkit-animation: rotateplane 3.2s infinite ease;
    animation: rotateplane 3.2s infinite ease;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
}
