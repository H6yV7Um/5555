<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
<meta content="yes" name="apple-mobile-web-app-capable">
<meta content="black" name="apple-mobile-web-app-status-bar-style">
<meta content="telephone=no,email=no" name="format-detection">
<link rel="stylesheet" type="text/css" href="css/style.css">
<title>星秀 明星给你送祝福</title>
</head>

<body>
<video src="" width="100%" height="100%" controls="controls" autoplay="autoplay" id="video" loop></video>
<script type="text/javascript" src="js/zepto.js"></script>
<script type="text/javascript">
var analyzParams = function(param_name) {
	var url = window.location.search.split("?")[1];
	if (url == "" || url == undefined) return url;
	url = url.split(param_name + "=")[1];
	if (url == "" || url == undefined) {
		url = "";
		return url;
	}
	if (url.indexOf("&") > 0) {
		url = url.split("&")[0];
	}
	return url;
}
var id = analyzParams("id") == undefined ? "" : analyzParams("id");
$(function() {
	if(id==1){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/bishijing.mp4");
	}else if(id==2){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/guoshutong.mp4");
	}else if(id==3){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/jiaojunyan.mp4");
	}else if(id==4){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/milu.mp4");
	}else if(id==5){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/nanfulong.mp4");
	}else if(id==6){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/wuxin.mp4");
	}else if(id==7){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/zhangbo.mp4");
	}else if(id==8){
		$("#video").attr("src","https://starshow-file.b0.upaiyun.com/ID/zhangyishan.mp4");
	}
})
</script>
</body>
</html>
