/*message*/
.mes-con {
	height: 100%;
	overflow: auto;
}
.mes-title {
	padding: 30px 0 16px;
	font-size: 20px;
	font-weight: 600;
}
.mes-con ul li {
	padding: 18px 20px 22px;
	border: 1px solid #e4e4e4;
	background: #fff;
	margin-bottom: 8px;
}
.mes-con ul li .item-head {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	margin-bottom: 16px;
}
.mes-con ul li .item-head h4 {
	position: relative;
	font-size: 18px;
	font-weight: bold;
}
.unread {
	position: absolute;
	top: -4px;
	right: -11px;
	width: 7px;
	height: 7px;
	background: #f50c0c;
	border-radius: 50px;
}
.mes-con ul li .item-head span {
	font-size: 14px;
	color: #757575;
}
.mes-con ul li p {
	font-size: 14px;
}

/*help */
.help-page {
	width: 98%;
}
.help-list {
	width: 98%;
	padding: 1%;
	background: #fff;
	border: 1px solid #e4e4e4;
	margin-top: 20px;
	overflow: auto;
	border-radius: 5px;
}

.help-list ul li h4 {
	/* padding: 12px 0; */
	line-height: 40px;
	font-size: 16px;
	font-weight: bold;
	cursor: pointer;
}
.help-list ul li h4 i {
	display: inline-block;
	width: 8px;
	height: 9px;
	background: url(../../images/help-arrow.png) no-repeat 0 0;
	margin-right: 14px;
	-webkit-transition: all .6s;
    transition: all .6s;
}
.help-item dl {
	display: none;
	width: 98%;
	/* height: 400px; */
	background: #f7fcff;
	border: 1px solid #ddf2ff;
	padding: 1%;
	margin-left: 0.3%;
	border-radius: 5px;
}	
.help-item dl dd {
	font-size: 14px;
	line-height: 24px;
	margin-bottom: 14px;
}
.help-item.action h4 {
	color: #0175be;
}
.help-item.action h4 i{
	-webkit-transform: rotate(90deg);
    transform: rotate(90deg);
}
.profile-table {
	border-top: 1px solid #abbfdb;
	margin-bottom: 10px;
}
.profile-table td, .profile-table th {
	padding: 5px 15px;
	font-size: 12px;
	font-family: Verdana;
	color: #5e7fb2;
}
.profile-table th, .profile-table tr {
