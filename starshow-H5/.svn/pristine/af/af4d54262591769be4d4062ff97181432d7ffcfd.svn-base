.dsp_container{
	width: 98%;
	margin: 0 auto;
}
.dsp_tools{
	width: 100%;
	overflow: hidden;
}
.dsp_container  select{
	border: 1px solid #e4e4e4;
	padding:5px 25px 5px 5px;
	font-size:14px;
	background: url(../../images/icon-arraw-bottom.png) no-repeat right center #fff;
}
.txt_timerpicker{
	border: 1px solid #e4e4e4;
	padding:5px;
}
.dsp_loginInfo{
  padding: 0 5px;
  font-size: 14px;
  color: #898989;
}
.dsp_charts{
	clear: both;
	width: 100%;
	overflow: hidden;
	border: 1px solid #e4e4e4;
	background: #fff
    
}
.charts_title{
	width: 98%;
	margin: 10px auto;
	overflow: hidden;
}
.timer_tabs {
	background: #fff
}
.timer_tabs span{
	display: block;
	float:left;
	padding:5px 10px;
	border: 1px solid #e4e4e4;
	border-right: none;
}
.timer_tabs span.tab{
	cursor:pointer;
}
.timer_tabs span.tab_disabled{
	background: #efecec;
}
.timer_tabs span:last-child{
	border-right: 1px solid #e4e4e4;
}
.timer_tabs span.cur{
	background: #026db0;
	color: #fff
}
/*indexlist page*/
.money_info{
	width: 100%;
	overflow: hidden;
	margin: 20px auto;
}
.money_info li{
	float: left;
	display: inline;
	width: 24%;/*32.5%;*/
	padding:20px 10px;
	border: 1px solid #e4e4e4;
	background: #fff;
	box-sizing:border-box;
}
.money_info li:nth-child(2){
	margin: 0 1%;
}
.money_info li:nth-child(3){
	margin: 0 1%;
}
.money_info li img{
	float: left;
	margin-top: 2px;
	margin-left: 10px;
}
.money_info li .money_desc{
	float: left;
	    margin-left: 5px;
}
.date{
	width: 230px;
    float: left;
    border: 1px solid #e4e4e4;
    padding: 5px 25px 5px 5px;
   background: #fff;
    position: relative;
}
.date .form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control {
    cursor: pointer; 
}
.date input{
	cursor:pointer
}
.no_border_left{
	border-left: none;
}
.abs_calendar{
	position: absolute;
    top: 2px;
    font-size: 20px;
    right: 5px;
}
.date input{
	border: none;
}
.date .input-group-addon{
	border: none;
}
.btn_create_adv{
	background: #57a008;
	padding: 5px 20px;
	display: inline-block;
	color: #fff;
	font-size: 14px;
}
.btn_create_adv a{
	color: #fff;
	text-decoration: none;
}
.form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control{
	background:#fff;
}
.input-group-addon{
	background:#fff;
}

.dsp_table{
	width: 100%;
	margin-top: 10px;
	box-sizing:border-box;
	border: 1px solid #dadada;
	background: #fff;
	font-size: 14px;


}
.dsp_table tr.cur{
