50%{-webkit-transform:translate3d(-50%,-50%,0) rotateZ(180deg)}
100%{-webkit-transform:translate3d(-50%,-50%,0) rotateZ(360deg)}
}
@-webkit-keyframes scale-to-zero-with-center{0%{-webkit-transform:translate3d(-50%,0,0) scale(20);opacity:0}
50%{opacity:.3}
100%{-webkit-transform:translate3d(-50%,0,0) scale(1);opacity:1}
}
@-webkit-keyframes fade-in{0%{opacity:0}
100%{opacity:1}
}
@-webkit-keyframes jump{0%{-webkit-transform:translate3d(0,0,0)}
50%{-webkit-transform:translate3d(0,40%,0)}
100%{-webkit-transform:translate3d(0,0,0)}
}
@-webkit-keyframes spin{0%{-webkit-transform:rotateZ(0deg)}
100%{-webkit-transform:rotateZ(360deg)}
}
@-webkit-keyframes flicker{0%{opacity:0}
50%{opacity:1}
100%{opacity:0}
}
@-webkit-keyframes only-flip{0%{opacity:1;-webkit-transform:perspective(400px) rotateX(90deg)}
30%{-webkit-transform:perspective(400px) rotateX(-20deg)}
50%{-webkit-transform:perspective(400px) rotateX(20deg)}
80%{-webkit-transform:perspective(400px) rotateX(-10deg)}
90%{-webkit-transform:perspective(400px) rotateX(10deg)}
100%{opacity:1;-webkit-transform:perspective(400px) rotateX(0deg)}
}
@-webkit-keyframes arrow{0%{-webkit-transform:translate3d(0,62px,0);opacity:0}
60%{-webkit-transform:translate3d(0,12px,0);opacity:1}
100%{-webkit-transform:translate3d(0,0,0);opacity:0}
}
