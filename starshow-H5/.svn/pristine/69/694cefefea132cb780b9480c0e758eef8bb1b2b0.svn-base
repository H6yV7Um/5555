<body>
    <section class="pageMain" id="rankMain" style="display: none;">
        <a id="btn_download"><img src="../assets/images/download.png" width="100%" /></a>
        <!--打星榜头-->
        <section class="rankCons">
            <header>
                <div class="star-pic"><img src="" alt=""></div>
                <div class="star-info">
                    <div class="star-name">annay</div>
                    <div class="rank-info">
                        <p class="amount">打榜指数：<span>1234</span></p>
                        <p class="devote">粉丝贡献：<span>1234</span>次</p>
                    </div>
                </div>
            </header>
            <!-- 打榜滚动 -->
            <section class="rank-scroll-outer">
                <div class="rank-scroll">
                    <ul>
                        <!-- <li class="scroll-item">
                            <div class="scroll-user-pic">
                                <img src="" alt="">
