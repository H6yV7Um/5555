html,body{height:100%;font-family: "Microsoft Yahei";} 
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,
textarea,p,blockquote,th,td{margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;}
fieldset,img{border:0; vertical-align: middle;}
address,caption,cite,code,dfn,em,strong,th,var,optgroup{font-style:inherit;font-weight:inherit;}
del,ins{text-decoration:none;}li{list-style:none;}caption,th{text-align:left;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}q:before,q:after{content:'';}
abbr,acronym{border:0;font-variant:normal;}
sup{vertical-align:baseline;}
sub{vertical-align:baseline;}
legend{color:#000;}
img{vertical-align: middle}
input,button,textarea,select,optgroup,option{font-family:inherit;font-size:inherit;font-style:inherit;
    font-weight:inherit;}
input,button,textarea,select{*font-size:100%;}
input[type="button"], input[type="submit"], input[type="reset"],input[type="text"],input[type="search"],select,textarea{
-webkit-appearance: none;
outline: 0;
-webkit-tap-highlight-color:rgba(0,0,0,0)
}

.cb{clear: both;}
.none{display:none}
.vm{vertical-align: middle;}
.fl{float: left; display: inline;}
.fr{float: right;display: inline;}
.fb{font-weight: bold;}
.tc{text-align: center;}
.mt70{margin-top: 70px;}
.mt60{margin-top: 60px;}
.mt30{margin-top: 30px;}
.mt20{margin-top: 20px;}
.mt10{margin-top: 10px;}
.col_red{color: #eb1010}

/* header */
.tips{
    position: absolute;
    left: -50px;
    display: none;
    top: 40px;
}

.cangp_main{
 font-family: "Microsoft Yahei";
 width:100%;
 min-width:1200px;
 min-height: 500px;
 overflow: hidden;
}
.cangp_header{
    background: #eb1010;
    width:100%;
    min-width: 1200px;
    height:81px;
    line-height: 80px;
    /* position: fixed;
    z-index: 1000; */
    left:0;
    right:0;
}
.cangp_content{
    position: relative;
    width: 1200px;
    margin: 0 auto;
    height: 80px;
    border: 1px solid transparent;
}
.icon_logo{
    float: left;
    margin-top: 10px;
}
.cangp_menus{
    float:right;
    color: #ffffff;
    font-size: 20px;
}
.cangp_nav{
    clear: both;
    margin-left: 30px;
}
/* .cangp_nav >li:first-child{
    display: none;
} */
.cangp_menus li{
    text-align: center;
    float: left;
    display: inline;
    padding: 0 15px;
}
.cangp_menus li a{
    color: #ffffff;
    text-decoration: none;
    position: relative;
}
.cangp_menus .line{
   width:100%;
    height: 2px;
    bottom: -15px;
    left: 0;
    right: 0;
    margin: auto;
    position: absolute;
    background: #fff;
    display:none
}
.cangp_menus li li{
    line-height: 30px;
    height: 30px;
}
.childsMain {
    width: 100%;
    background: rgba(0,0,0,0.5);
    width: 100%;
    position: absolute;
    margin-top:-2px;
    left: 0;
    z-index: 999;
    color: #fff;
    text-align: center;
}
.childsMain  li a{
    color: #fff
}
.menu_childs{
    width: 1200px;
    position: static;
    opacity: 0.8;
    padding: 8px 0;
    margin: 0 auto;
    overflow: hidden;
    background: none;
}
/* .menu_childs{
    width: 100%;
    position: absolute;
    top: 44px;
    left:0;
    z-index: 999;
    color: #000;
    background: #f3f3f3;
    opacity: 0.8;
    padding: 5px 0;

} */
.menu_childs li a{
  color:#fff
}
.menu_childs li.cur a{
    color: #ff1d3e
}
.cangp_other{
    width:400px;
    float: right;
    font-size: 14px;
    text-align: right;
    position: relative;
}
.cangp_other span{
  margin: 0 10px;
}
.cangp_other a:last-child{
    /* margin-left: 30px; */
}
.change-lang {
    position: absolute;
    left: 160px;
    height: 80px;
    color: #fff;
    margin-left: 15px;
    cursor: pointer;
}
.checked-lang i {
    display: inline-block;
    width: 9px;
    height: 9px;
    background: url(../images/down-arrow.png) no-repeat 0 0;
    margin-left: 8px;
}

.lang-list {
    display: none;
    position: absolute;
    top: 60px;
    left: -14px;
    width: 100px;
    background: #fff;
    z-index: 10;
    box-shadow: 0px 0px 8px rgba(0,0,0,.3);
}
.change-lang:hover .lang-list {
    display: block;
}
.lang-list li {
    line-height: 50px;
    text-align: center;
}
.lang-list li a {
    display: block;
    width: 100%;
    height: 100%;
}
.lang-list li:hover a{
    background: #eb1010;
    color: #fff;
}
.cangp_cons{
    width:1000px;
    margin-top: 85px;
    overflow: hidden;
    margin-left:auto;
    margin-right:auto;
}
.cangp_cons.mt70{
    margin-top: 90px;
}
/*crumb*/
.cangp_crumb{
    height: 50px;
    line-height: 50px;
    color: #8a8a8a;
    font-size: 14px;
}
/*footer*/
.cangp_footer{
    width:100%;
    min-width:1200px;
    background: #221f1f;
    color: #bdbdbd;
    line-height:200%;
    font-size: 12px;
    padding: 30px 0 10px 0;
    overflow: hidden;
}
.cangp_footer_con{
    width: 1200px;
    overflow: hidden;
    margin: 0 auto;
}
.footer_cen {
    width: 860px;
    overflow: hidden;
    margin-top:20px;
    color: #6b6b6b;
    /* border-right: 1px solid #292929; */
    font-size:18px;
    color: #f1f1f1;
    line-height: 30px;
    font-family: "黑体";
}
.footer_info{
  margin-left:30px;
}
.footer_cen ul{
    width: 100%;
    overflow: hidden;
    margin:0 auto;
    font-size: 14px;
}
.footer_cen li{
    width:25%;
    float: left;
    display: inline;

}
.cangp_footer li a{
    text-decoration: none;
    color: #bdbdbd;
    margin-top: 10px;
}
.footer_right{
    float: right;
    display: inline;
}
.footer_right li{
    float: left;
    display: inline;
    margin-left: 14px;
    text-align: center;
    font-size: 12px;
    color: #6b6b6b
}
