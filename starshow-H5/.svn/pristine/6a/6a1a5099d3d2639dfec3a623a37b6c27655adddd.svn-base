.swiper-button-next {
    background-image: url(../images/icon-right3.png);
}
.swiper-button-prev {
    background-image: url(../images/icon-left3.png);
}
.swiper-button-next, .swiper-button-prev {
   width:31px; height: 31px;
   background-size: 31px 31px;
}
.swiper-button-next.type2, .swiper-button-prev.type2{
   width:58px; height: 58px;
   background-size: 58px 58px;
}
.swiper-button-next.type3, .swiper-button-prev.type3{
   top:40%
}
.swiper-button-prev.type2{
      background-image: url("../images/icon/btn_left2.png")
}
.swiper-button-next.type2{
      background-image: url("../images/icon/btn_right2.png")
}
.swiper-button-prev.type3{
      background-image: url("../images/icon/btn_left3.png")
}
.swiper-button-next.type3{
      background-image: url("../images/icon/btn_right3.png")
}
.swiper-pagination-bullet{
    width:10px;height: 10px;
}
.swiper-pagination-bullet-active{
    background: #ff1d3e
}
.swiper-container-horizontal>.swiper-pagination-bullets{
        bottom: 30px;
}
/*audio animate css*/
/*add content--audio style*/
.audio_main{
    width:20%;
    background:#fff3ea;
    border-radius:5px;
    line-height:2rem;
    height:2rem;
    margin-top:10px;
    margin-left:10px;
    position: relative;
    overflow:hidden;
 }
.audio_yuyin {
    background: url(../../assets/images/yuyin-s.png) 0 0 no-repeat;
    width: 20px;
    margin-top: 5px;
    float: right;
    margin-right: 10px;
        height: 20px;
}

.move{
    animation: animate-tail 1s steps(4) infinite;
}
@keyframes animate-tail {  
     0% {background-position: 0 0;}
     100% {background-position: -85px 0; }
}




