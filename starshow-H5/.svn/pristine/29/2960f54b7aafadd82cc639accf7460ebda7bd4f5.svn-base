				num = $(this).index();
				var mySwiper = new Swiper ('.swiper-container', {
					direction:"horizontal",
					initialSlide:num,
				})
			})
			$(".XXX").on("click",function(){
				$("#dot").css("display","none")
				$("#video_back").css("display","block")
			})
