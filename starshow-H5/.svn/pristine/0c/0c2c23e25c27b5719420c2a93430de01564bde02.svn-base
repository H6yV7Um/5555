/*! starshow 2016-03-15 */
* {
	margin: 0;
	padding: 0;
}
body {
	margin: 0;
	padding: 0;
	background: url(../images/bg.jpg) center 82px no-repeat;
}
html {
	background: url(../images/htmlBg.jpg) center bottom no-repeat;
}
body {
	font-family: arial, sans-serif;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after, q:before, q:after {
	content: '';
	content: none;
}
ins, a {
	text-decoration: none;
}
del {
	text-decoration: line-through;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
img {
	border: 0;
}
.clear {
	clear: both;
}
.lm_header {
	width: 980px;
	height: 80px;
	line-height: 80px;
	margin: 0 auto;
	box-sizing: border-box;
}
.lm_header li {
	float: left;
}
.lm_header .lm_header_left {
	font-size: 30px;
	width: 230px;
	box-sizing: border-box;
}
.lm_header .lm_header_left img {
	width: 60px;
	vertical-align: middle;
	margin-right: 20px;
}
.lm_header .lm_header_right {
	margin-left: 553px;
	width: 425px;
	text-align: right;
	box-sizing: border-box;
}
.lm_header .lm_header_right a {
	font-size: 20px;
	color: #000;
	padding: 0 45px;
}
.lm_header .lm_header_right .three {
	padding-right: 0;
}
.lm_down {
	width: 980px;
	margin: 0 auto;
	position: relative;
	height: 730px;
}
.lm_down img {
	float: left;
}
.lm_down .lm_down_con {
	position: absolute;
	left: 595px;
	z-index: 2;
	top: 425px;
	width: 300px;
	text-align: center;
}
.lm_down .lm_down_con h2 {
	font-size: 70px;
	text-align: left;
}
.lm_down .lm_down_con h3 {
	font-size: 25px;
	text-align: left;
}
.lm_down .lm_down_con p {
	width: 288px;
	height: 64px;
	line-height: 64px;
	border-radius: 38px;
}
.lm_down .lm_down_con .ios {
	background-color: #feed3c;
	border: 2px solid #f7eb41;
	margin-bottom: 17px;
}
.lm_down .lm_down_con .phone {
	background-color: #f3d53c;
	border: 2px solid #f7eb41;
}
.lm_down .lm_down_con a {
	font-size: 22px;
	color: #c58e0a;
}
.lm_down .lm_down_con .phone a:before {
	content: "";
	display: inline-block;
	background: url('../images/web2.png');
	background-position: -1px 54px;
	width: 30px;
	height: 55px;
	margin-right: 10px;
	vertical-align: -12px;
}
.lm_down .lm_down_con .ios a:before {
	content: "";
	display: inline-block;
	background: url('../images/web2.png');
	background-position: 0 0;
	width: 30px;
	height: 55px;
	margin-right: 10px;
	vertical-align: -27px;
}
.abautus {
	width: 980px;
	margin: 48px auto 0;
	text-align: center;
	color: #000;
	font-size: 18px;
	line-height: 26px;
	overflow: hidden
}
.abautus a {
	color: #737373;
}
.abautus h2, .abautus h3 {
	color: #000;
	font-size: 30px;
	padding: 30px 0;
	font-weight: bold;
}
.abautus h3 {
	padding: 0;
	padding-top: 169px;
	color: #fff
}
.abautus p {
	line-height: 32px;
}
.lm_concat_con {
	margin: 0 auto;
	background: #737373;
	z-index: 2;
	width: 980px;
	text-align: center;
	color: #fff;
	font-size: 18px;
	padding-top: 37px;
}
.lm_concat_con h2 {
	font-size: 30px;
	margin-bottom: 57px;
}
.lm_concat_con .erweima {
	border-radius: 52px;
	margin-left: 70px;
	width: 174px;
}
.lm_concat_con .phone {
	margin: 30px 0 18px 0;
}
.lm_concat_con .belong {
	height: 60px;
	line-height: 60px;
	margin-top: 10px;
}
